{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) tests",
      "type": "codelldb",
      "request": "launch",
      "program": "./zig-out/build/test",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "preLaunchTask": "Build Tests",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
